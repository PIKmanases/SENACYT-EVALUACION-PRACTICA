<script>
    function ocultarMenu(){
        var item = document.getElementById("leftMenu");
        if(item.classList.contains( 'd-none' )){
            document.getElementById("leftMenu").classList.remove("d-none");
        }else{
            document.getElementById("leftMenu").classList.add("d-none");
        }

    }
</script>